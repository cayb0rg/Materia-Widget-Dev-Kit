<link rel='stylesheet' href='/mwdk/assets/css/mwdk-datatable.css' type='text/css' />
<link rel="stylesheet" href="/mwdk/assets/css/ng-modal.css" type="text/css" />

<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
<script src="//cdn.datatables.net/1.10.12/js/jquery.dataTables.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.6.9/angular.min.js"></script>
<script src="/mwdk/assets/js/materia.js"></script>
<script src="/mwdk/assets/js/author.js"></script>

<div ng-app="materia">
	<div style="width:100%" ng-controller="questionImporterCtrl">
		<form id="import-form">
			<h1>Question Catalog</h1>
			<table id="question-table" width="100%">
				<thead width="100%">
					<tr>
						<th>Question Text</th>
						<th>Type</th>
						<th>Date</th>
						<th>Used</th>
					</tr>
				</thead>
			</table>
			<div class="actions">
				<a id="cancel-button" href="#" onClick="self.close();">Cancel</a>
				<input id="submit-button" type="button" class="action_button" value="Import Selected">
			</div>
		</form>
	</div>
</div>
